{
  "engines": {
    "node": ">=20 <21"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "ajv-cli": "^5.0.0",
    "chrome-launcher": "^1.2.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.1.6",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.55.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "yazl": "^3.3.1"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "express": "^5.1.0",
    "yaml": "^2.8.1"
  },
  "scripts": {
    "mock:staging": "node mock/server.js",
    "deliver": "node orchestration/cli.mjs",
    "deliver:0003": "node orchestration/cli.mjs AUV-0003",
    "deliver:0002": "node orchestration/cli.mjs AUV-0002",
    "validate:cards": "node ./node_modules/ajv-cli/dist/index.js validate -s schemas/runbook-summary.schema.json -d \"runs/**/result-cards/runbook-summary.json\" --verbose",
    "validate:brief": "npx ajv validate -s contracts/brief.schema.json --spec=draft7 --verbose -d",
    "plan": "node orchestration/cli.mjs plan",
    "plan:demo": "node orchestration/cli.mjs plan briefs/demo-01/brief.md",
    "plan:demo:dry": "node orchestration/cli.mjs plan briefs/demo-01/brief.md --dry-run",
    "test:unit": "node --test --test-reporter=tap --test-timeout=30000 tests/unit",
    "test:unit:sequential": "node tests/unit/run-all.mjs",
    "test:unit:native": "node --test tests/unit",
    "test:integration": "node --test tests/integration",
    "test:router": "node tests/router.test.mjs",
    "router:dry": "node mcp/router.mjs --dry --input mcp/router-fixtures/primary-only.json",
    "router:dry:security": "node mcp/router.mjs --dry --input mcp/router-fixtures/security-request.json",
    "router:dry:secondary": "node mcp/router.mjs --dry --input mcp/router-fixtures/secondary-with-consent.json",
    "router:test": "node mcp/router.mjs --dry --agent B7.rapid_builder --capabilities browser.automation,web.perf_audit --budget 0.25",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "qa": "npm run format:check && npm run lint && npm run typecheck && npm run test:unit && npm run test:integration",
    "build-lane": "node orchestration/cli.mjs build-lane",
    "validate:manifest": "node ./node_modules/ajv-cli/dist/index.js validate -s schemas/manifest.schema.json -d \"dist/*/manifest.json\" --verbose",
    "validate:manifest:strict": "node ./node_modules/ajv-cli/dist/index.js validate -s schemas/manifest.schema.json -d \"dist/*/manifest.json\" --verbose --strict --all-errors",
    "package": "node orchestration/cli.mjs package",
    "report": "node orchestration/cli.mjs report",
    "deliver:full": "node orchestration/cli.mjs deliver",
    "prepare": "husky",
    "pre-push": "npm run lint && npm run typecheck && npm run test:unit"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ],
    "*.{json,html,css,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
