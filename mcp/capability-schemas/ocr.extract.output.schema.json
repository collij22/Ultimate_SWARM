{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "capability://ocr.extract.output@v1",
  "title": "ocr.extract output",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "text": { "type": "string" },
    "bbox": {
      "type": "array",
      "items": { "type": "number" },
      "minItems": 4,
      "maxItems": 4
    },
    "fields": {
      "type": "object",
      "additionalProperties": {
        "type": ["string", "number", "null"]
      }
    }
  },
  "required": ["text"]
}


